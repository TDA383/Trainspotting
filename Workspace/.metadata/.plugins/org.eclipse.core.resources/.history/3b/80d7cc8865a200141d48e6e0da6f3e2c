import java.util.concurrent.*;
import TSim.*;

public class TrainSemaphore extends Semaphore {

	private final Train train;

	public TrainSemaphore(int permits, Train train) {
		super(permits);
		this.tsi = tsi;
	}
	
	@Override
	public void acquire() {
		train.changeSpeed(0);
	}
	
	@Override
	public void release() {
		
	}
}
